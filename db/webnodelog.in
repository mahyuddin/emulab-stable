#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webnodelog arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/sbin/nodelog", @ARGV;

die("webnodelog: Could not exec nodelog: $!");
