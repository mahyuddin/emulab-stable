use English;

#
# This gets invoked from the Web interface. 
# Simply a wrapper for delay node configuration.
#
# usage: webdelay_config arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/delay_config", @ARGV;

die("webdelay_config: Could not exec delay_config: $!");
