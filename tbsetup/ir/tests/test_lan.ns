set ns [new Simulator]

set node0 [$ns node]
set node1 [$ns node]
set node2 [$ns node]
set node3 [$ns node]
set node4 [$ns node]
set node5 [$ns node]
set node6 [$ns node]
set node7 [$ns node]

set mylan0 [$ns make-lan "$node0 $node1" 100Mb .1ms]
set mylan1 [$ns make-lan "$node2 $node3" 100Mb .1ms]
set mylan2 [$ns make-lan "$node4 $node5" 100Mb .1ms]
set mylan3 [$ns make-lan "$node6 $node7" 100Mb .1ms]

$ns duplex-link $mylan0 $mylan1 100Mb .1ms DropTail 
$ns duplex-link $mylan1 $mylan2 100Mb .1ms DropTail
$ns duplex-link $mylan2 $mylan3 100Mb 100ms DropTail

$ns run

