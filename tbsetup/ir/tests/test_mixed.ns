set ns [new Simulator]

set node0 [$ns node]
set node1 [$ns node]
set node2 [$ns node]
set node3 [$ns node]
set node4 [$ns node]

set mylan0 [$ns make-lan "$node0 $node1" 100Mb .1ms]

$ns duplex-link $mylan0 $node2 100Mb .1ms DropTail 
$ns duplex-link $mylan0 $node3 100Mb .1ms DropTail
$ns duplex-link $node3 $node4 100Mb 100ms DropTail

$ns run

