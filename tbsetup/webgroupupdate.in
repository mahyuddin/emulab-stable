#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webgroupupdate arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/sbin/group-update", @ARGV;

die("webgroupupdate: Could not exec group-update: $!");
