#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper.
#
# usage: webkillbatchexp arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/killbatchexp", @ARGV;

die("webkillbatchexp: Could not exec killbatchexp: $!");
