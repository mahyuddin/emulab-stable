#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper for experiment
# creation.
#
# usage: webstartexp arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/startexp", @ARGV;

die("webstartexp: Could not exec startexp: $!");
