#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webmkacct arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/sbin/mkacct", @ARGV;

die("webmkacct: Could not exec mkacct: $!");
