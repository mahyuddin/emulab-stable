use English;

#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webrmuser arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/sbin/rmuser", @ARGV;

die("webrmuser: Could not exec rmuser: $!");
