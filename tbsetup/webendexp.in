#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper for experiment
# termination.
#
# usage: webendexp arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/endexp", @ARGV;

die("webendexp: Could not exec endexp: $!");
