#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webnodeupdate pid eid
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/node_update", @ARGV;

die("webnodeupdate: Could not exec node_update: $!");
