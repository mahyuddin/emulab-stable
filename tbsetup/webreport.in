#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper for tbreport.
#
# usage: webreport <pid> <eid>
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/tbreport", @ARGV;

die("webreport: Could not exec tbreport: $!");
