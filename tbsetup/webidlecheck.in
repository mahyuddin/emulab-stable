#!/usr/bin/perl -w
use English;


# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webidlecheck arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/idlecheck", @ARGV;

die("webidlecheck: Could not exec idlecheck: $!");
