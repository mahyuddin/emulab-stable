#!/usr/bin/perl -w
use English;

#
# This gets invoked from the Web interface. Simply a wrapper ...
#
# usage: webvistopology arguments ...
#

#
# Configure variables
#
my $TB       = "@prefix@";

#
# Run the real thing, and never return.
# 
exec "$TB/bin/dbvistopology", @ARGV;

die("webvistopology: Could not exec dbvistopology: $!");
