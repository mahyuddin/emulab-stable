#!/bin/sh
#
# Utah Network Testbed plab startup
#
if [ -x @prefix@/sbin/plabdaemon  ]; then
        echo -n " plabrenew"
        @prefix@/sbin/plabdaemon -v renew
        echo -n " plabgetfree"
        @prefix@/sbin/plabdaemon -v getfree
fi

if [ -x @prefix@/sbin/plabmonitord  ]; then
        echo -n " plabmonitord"
        @prefix@/sbin/plabmonitord
fi
