#! /bin/sh


# setup logging
if [ $1 ] ; then
    logfile=$1
else
    logfile="/tmp/nodecheck.log"
fi
tmplog=/tmp/.$$.log
cat /dev/null > ${tmplog}

set +x
#exit on unbound var
#set -u

echo "memtest `date`" >> ${logfile}
cat ${tmplog} >> ${logfile} 

if [ -n ${failed} ]
then
    echo ". . . memtest OK"
else
    echo ". . . memtest FAILED"
fi
echo -n ""

exit 0
