SRCDIR		= @srcdir@
TESTBED_SRCDIR	= @top_srcdir@
OBJDIR		= ..
SUBDIR		= event

include $(OBJDIR)/Makeconf

SUBDIRS	= lib sched tbgen example

all:	$(SUBDIRS)

include $(TESTBED_SRCDIR)/GNUmakerules

lib:
	@$(MAKE) -C lib all

sched:
	@$(MAKE) -C sched all

tbgen:
	@$(MAKE) -C tbgen all

example:
	@$(MAKE) -C example all

install: 
	@$(MAKE) -C lib install
	@$(MAKE) -C sched install
	@$(MAKE) -C tbgen install

clean:		subdir-clean

subdir-clean:
	@$(MAKE) -C lib clean
	@$(MAKE) -C sched clean
	@$(MAKE) -C tbgen clean
	@$(MAKE) -C example clean

distclean:	subdir-distclean

subdir-distclean:
	@$(MAKE) -C lib distclean
	@$(MAKE) -C sched distclean
	@$(MAKE) -C tbgen distclean
	@$(MAKE) -C example distclean

.PHONY:	$(SUBDIRS)

