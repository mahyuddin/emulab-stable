default namespace = "http://www.protogeni.net/resources/peers/1"

include "any-extension.rnc"

Peers = element peers {
  AnyExtension & 
  Authority*
}

Authority = element authority {
  AnyExtension &
  attribute name { text } &
  attribute url { text } &
  attribute permit { "cm" | "sa" | "ses" | "ch" | "am" | "id" }
}

start = Peers
