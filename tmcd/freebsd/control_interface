# All rights reserved.
#

NODETYPE=`/etc/testbed/nodetype`

if [ "$NODETYPE" = 'pc600' ]; then
    echo 'fxp4'
elif [ "$NODETYPE" = 'pc850' ]; then
    echo 'fxp0'
elif [ "$NODETYPE" = 'pcL440GX' ]; then
    echo 'fxp1'
elif [ "$NODETYPE" = 'pc1500' ]; then
    echo 'xl0'
elif [ "$NODETYPE" = 'pc2000' ]; then
    echo 'xl0'
else
    echo 'unknown'
fi
