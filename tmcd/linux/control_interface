. /etc/emulab/paths.sh

NODETYPE=`$BINDIR/nodetype`
case $NODETYPE in
pc300|pc1500|pc2000|pc3000|pc2400-aero|pc2400-orbit|pc733|pcL440GX)
    echo 'eth0'
    ;;
i2)
    echo 'eth1'
    ;;
pc850)
    echo 'eth2'
    ;;
pc600|pc850a)
    echo 'eth4'
    ;;
*)
    echo 'unknown';
    ;;
esac
