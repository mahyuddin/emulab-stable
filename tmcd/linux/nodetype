#!/bin/sh
#
# EMULAB-COPYRIGHT
# Copyright (c) 2000-2002 University of Utah and the Flux Group.
# All rights reserved.
#
. /etc/emulab/paths.sh

if [ -x $BINDIR/cpuspeed.awk ]; then
	cpuspeed=`$BINDIR/cpuspeed.awk /proc/cpuinfo`;
else
	cpuspeed='';
fi

if [ x$cpuspeed = x ] || [ $cpuspeed = 0 ]; then
	echo "unknown";

# XXX hack for minibed
elif [ $cpuspeed = "850" -a -x $BINDIR/chipset.awk ]; then
	if [ "`$BINDIR/chipset.awk /proc/pci`" = "GX" ]; then
		echo pcL440GX;
	else
		echo pc$cpuspeed;
	fi

# For Wiconsin nodes - they have a different node typing scheme
elif [ $cpuspeed = "2500" ]; then
	echo i2;

else
	echo pc$cpuspeed;
fi
