DEVICE="eth2"

. /etc/emulab/paths.sh

if [ "`control_interface`" = "$DEVICE" ]; then
	BOOTPROTO="dhcp"
	NEEDHOSTNAME=""
	hostname emulab-localhost
fi
ONBOOT="yes"
IPADDR=""
NETMASK=""
IPXNETNUM_802_2=""
IPXPRIMARY_802_2="no"
IPXACTIVE_802_2="no"
IPXNETNUM_802_3=""
IPXPRIMARY_802_3="no"
IPXACTIVE_802_3="no"
IPXNETNUM_ETHERII=""
IPXPRIMARY_ETHERII="no"
IPXACTIVE_ETHERII="no"
IPXNETNUM_SNAP=""
IPXPRIMARY_SNAP="no"
IPXACTIVE_SNAP="no"
